{
  "$schema": "https://json.schemastore.org/package.json",
  "private": true,
  "funding": "https://jaywcjlove.github.io/#/sponsor",
  "homepage": "https://uiwjs.github.io/react-amap",
  "scripts": {
    "prepare": "husky install",
    "build": "lerna exec \"tsbb build \\\"src/*.{tsx,ts}\\\" --use-babel --bail\" --scope @lbh7/* --ignore @lbh7/react-amap-types",
    "bundle": "lerna exec \"ncc build src/index.tsx --target web --filename amap\" --scope @lbh7/react-amap",
    "bundle:watch": "lerna exec \"ncc watch src/index.tsx --target web --filename amap\" --scope @lbh7/react-amap",
    "bundle:min": "lerna exec \"ncc build src/index.tsx --target web --filename amap --minify\" --scope @lbh7/react-amap",
    "⬇️⬇️⬇️⬇️⬇️ watch ⬇️⬇️⬇️⬇️⬇️": "▼▼▼▼▼ watch::package ▼▼▼▼▼",
    "watch:amap": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap",
    "watch:api-loader": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-api-loader",
    "watch:auto-complete": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-auto-complete",
    "watch:bezier-curve": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-bezier-curve",
    "watch:circle": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-circle",
    "watch:circle-marker": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-circle-marker",
    "watch:context-menu": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-context-menu",
    "watch:control-bar-control": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-control-bar-control",
    "watch:ellipse": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-ellipse",
    "watch:geolocation": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-geolocation",
    "watch:hawk-eye-control": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-hawk-eye-control",
    "watch:info-window": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-info-window",
    "watch:map": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-map",
    "watch:map-type-control": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-map-type-control",
    "watch:tile-layer": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-tile-layer",
    "watch:marker": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-marker",
    "watch:label-marker": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-label-marker",
    "watch:mass-marks": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-mass-marks",
    "watch:mouse-tool": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-mouse-tool",
    "watch:poly-editor": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-poly-editor",
    "watch:polygon": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-polygon",
    "watch:polygon-editor": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-polygon-editor",
    "watch:polyline": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-polyline",
    "watch:polyline-editor": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-polyline-editor",
    "watch:rectangle": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-rectangle",
    "watch:require-script": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-require-script",
    "watch:scale-control": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-scale-control",
    "watch:text": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-text",
    "watch:tool-bar-control": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-tool-bar-control",
    "watch:utils": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-utils",
    "watch:weather": "lerna exec \"tsbb watch \\\"src/*.{tsx,ts}\\\" --use-babel\" --scope @lbh7/react-amap-weather",
    "⬆️⬆️⬆️⬆️⬆️ watch ⬆️⬆️⬆️⬆️⬆️": "▲▲▲▲▲ watch::package ▲▲▲▲▲",
    "type-check": "lerna exec \"tsc --noEmit\" --scope @lbh7/*",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage --bail",
    "version": "lerna version --exact --force-publish --no-push --no-git-tag-version",
    "bootstrap": "lerna bootstrap",
    "hoist": "lerna bootstrap --hoist",
    "doc:build": "lerna exec \"npm run build\" --scope website",
    "start": "lerna exec \"npm run start\" --scope website",
    "prettier": "prettier --write '**/*.{js,jsx,tsx,ts,less,md,json}'",
    "remove": "npm run clean && lerna exec \"rm -rf package-lock.json\" --scope @lbh7/* --scope website",
    "clean": "lerna clean --yes"
  },
  "author": "Kenny Wong <wowohoo@qq.com>",
  "license": "MIT",
  "lint-staged": {
    "*.{js,jsx,ts,tsx,less,md,json}": [
      "prettier --write"
    ]
  },
  "workspaces": [
    "packages/**",
    "website"
  ],
  "engines": {
    "node": ">=16.0.0"
  },
  "jest": {
    "testMatch": [
      "<rootDir>/test/*.{ts,tsx}",
      "<rootDir>/packages/**/__tests__/*.{ts,tsx}"
    ],
    "collectCoverageFrom": [
      "<rootDir>/packages/**/*.{tsx,ts}",
      "!**/.kktrc.ts",
      "!**/*.{js,d.ts}"
    ]
  },
  "devDependencies": {
    "@types/react-test-renderer": "^18.0.0",
    "@kkt/less-modules": "^7.5.2",
    "@kkt/ncc": "^1.0.15",
    "jest": "^29.5.0",
    "jest-watch-typeahead": "^2.2.2",
    "jest-environment-jsdom": "^29.5.0",
    "react": "~18.2.0",
    "react-dom": "~18.2.0",
    "react-test-renderer": "^18.2.0",
    "cross-env": "~7.0.3",
    "husky": "^8.0.3",
    "prettier": "^3.0.0",
    "lint-staged": "^15.1.0",
    "lerna": "^8.0.0",
    "tsbb": "^4.1.14"
  }
}